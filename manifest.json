{
  "author": "igorlogius",
  "icons": {
    "266": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "commands": {
    "_execute_browser_action": {}
  },
  "description": "Detect feeds (rss,atom,json,...) on any websites, with the click on a button",
  "homepage_url": "https://github.com/igorlogius/find-feeds",
  "manifest_version": 2,
  "name": "Find Feeds",
  "options_ui": {
    "open_in_tab": true,
    "page": "options.html"
  },
  "permissions": ["storage", "<all_urls>"],
  "version": "1.2.5"
}
